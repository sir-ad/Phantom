{
  "name": "@phantom-pm/cli",
  "version": "3.0.1",
  "description": "PHANTOM - The invisible force behind every great product. Open-source PM operating system for the terminal age.",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "phantom": "dist/index.js"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.tsx",
    "start": "node dist/index.js",
    "postinstall": "node dist/postinstall.js || exit 0",
    "prepublishOnly": "npm run build"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@inquirer/prompts": "^8.2.1",
    "@phantom-pm/core": "3.0.1",
    "@phantom-pm/mcp-server": "3.0.1",
    "@phantom-pm/tui": "3.0.1",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "conf": "^13.0.1",
    "express": "^5.2.1",
    "figlet": "^1.7.0",
    "ink": "^5.2.1",
    "ink-spinner": "^5.0.0",
    "inquirer": "^9.2.0",
    "open": "^11.0.0",
    "ora": "^8.0.1",
    "react": "^18.3.1",
    "yargs": "^18.0.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/figlet": "^1.5.8",
    "@types/ink": "^0.5.2",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.14.0",
    "@types/open": "^6.1.0",
    "@types/react": "^18.3.28",
    "tsx": "^4.16.0",
    "typescript": "^5.5.0"
  },
  "overrides": {
    "react": "$react",
    "ink": {
      "react": "$react"
    }
  },
  "keywords": [
    "product-management",
    "ai",
    "cli",
    "pm",
    "agent",
    "terminal",
    "product",
    "development",
    "mcp",
    "automation"
  ],
  "author": "PhantomPM <hello@phantom.pm>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sir-ad/Phantom.git",
    "directory": "packages/cli"
  },
  "homepage": "https://phantom.pm",
  "bugs": {
    "url": "https://github.com/PhantomPM/phantom/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
